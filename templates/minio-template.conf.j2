MINIO_ROOT="/srv/minio/{{ item.key }}"
# Local export path.
MINIO_VOLUMES="/srv/minio/{{ item.key }}/data/"
# Access Key of the server.
MINIO_ACCESS_KEY="{{ item.value.access_key }}"
# Secret key of the server.
MINIO_SECRET_KEY="{{ item.value.secret_key }}"
# Use if you want to run Minio on a custom port.
MINIO_OPTS='{{ item.value.server_opts }}'
#MINIO_OPTS='--address :9199'
